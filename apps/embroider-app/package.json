		"stylelint": "14.14.0",
		"stylelint-config-hudochenkov": "8.0.0",
		"stylelint-config-prettier": "9.0.3",
		"stylelint-config-standard-scss": "6.0.0",
		"stylelint-order": "5.0.0",
